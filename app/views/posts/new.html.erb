<section class = "section">
<%= form_with model: @post, url: user_posts_path , class: 'form' do |form| %>
<div>
    <%= form.label :title, "title:" %>
    <%= form.text_field :title, class: ''%>
</div>
<%= form.label :text, " " %>
<%= form.text_area :text, class: '', rows: 10 %>
<%= form.submit "post", class: '' %>
<% end %>
</section>